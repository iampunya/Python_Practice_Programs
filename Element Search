import random

list_length = 10

random_list = [random.randint(1, 100) for _ in range(list_length)]
print(random_list)
ordered_list = sorted(random_list)

random_number = random.randint(1, 10)
print(random_number)

if random_number in random_list:
    print("number is present!")
else:
    print("number is absent!")
    
def number_in_list(random_number,random_list):
    return(random_number in random_list)
    
result = number_in_list(random_number,random_list)
print(result)
